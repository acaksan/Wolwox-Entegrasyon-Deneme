# Test ortamı ayarları
ENVIRONMENT=test

# API Ayarları
API_HOST=localhost
API_PORT=8001
API_PREFIX=/api/v1
API_DEBUG=true

# Redis Ayarları
REDIS_URL=redis://localhost:6379/1
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=1
REDIS_PASSWORD=Ac1476ac.!
REDIS_CACHE_PREFIX=wolvox_test
REDIS_CACHE_TTL=60
REDIS_CACHE_ENABLED=true

# Firebird Test Ayarları
FIREBIRD_HOST=localhost
FIREBIRD_DATABASE=test.fdb
FIREBIRD_USER=test_user
FIREBIRD_PASSWORD=test_password
FIREBIRD_PORT=3050
FIREBIRD_CHARSET=UTF8
FIREBIRD_POOL_SIZE=2

# WooCommerce Test Ayarları
WOOCOMMERCE_URL=http://localhost:8000
WOOCOMMERCE_KEY=test_key
WOOCOMMERCE_SECRET=test_secret
WOOCOMMERCE_API_VERSION=wc/v3
WOOCOMMERCE_VERIFY_SSL=false
WOO_MAX_CONNECTIONS=10

# WordPress Test Ayarları
WORDPRESS_USERNAME=test_admin
WORDPRESS_PASSWORD=test_password

# Log Ayarları
LOG_LEVEL=DEBUG
LOG_FILE=logs/test.log
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Metrik Ayarları
ENABLE_METRICS=true
METRICS_PORT=9091
METRICS_HOST=localhost
METRICS_PATH=/metrics

# Cache Ayarları
CACHE_PREFIX=wolvox_test:
CACHE_TTL=60
CACHE_ENABLED=true

# Stok Ayarları
LOW_STOCK_THRESHOLD=5
STOCK_CACHE_TTL=60

# Test Veritabanı Ayarları
DB_HOST=localhost
